{
  "name": "standardnotes-todo-editor",
  "version": "0.0.1",
  "description": "A proof-of-concept todo.txt-like editor for Standard Notes",
  "author": "Colin Drake",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cfdrake/standardnotes-todo-editor"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules && mkdir -p build && cp index.html build/index.html && mv dist build/dist",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "start": "cross-env NODE_ENV=development webpack-dev-server --open --hot"
  },
  "dependencies": {
    "vue": "^2.2.1"
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-preset-latest": "^6.0.0",
    "cross-env": "^3.0.0",
    "css-loader": "^0.25.0",
    "file-loader": "^0.9.0",
    "node-sass": "^4.5.2",
    "pug": "^2.0.0-beta.12",
    "pug-loader": "github:pugjs/pug-loader",
    "sass": "^0.5.0",
    "sass-loader": "^6.0.3",
    "vue-loader": "^11.1.4",
    "vue-template-compiler": "^2.2.1",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^2.2.0"
  }
}
